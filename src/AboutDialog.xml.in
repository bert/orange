    <property name="border-width">15</property>
    <!--<property name="width-request">640</property>
    <property name="height-request">480</property>-->

    <property name="program-name">@PACKAGE_NAME@</property>
    <property name="version">@PACKAGE_VERSION@</property>
    <property name="website">@PACKAGE_URL@</property>
    <property name="website-label">@PACKAGE_URL@</property>
